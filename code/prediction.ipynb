   "source": [
    "def predict_peptide_class(peptide_seq):\n",
    "    input_seq_tok = tokenizer(peptide_seq, return_tensors='pt')\n",
    "    output = model(**input_seq_tok)\n",
    "    logits = output[0]\n",
    "\n",
    "    y_prob = torch.sigmoid(logits)[:, 1].detach().numpy()\n",
    "    y_pred = y_prob > 0.5\n",
    "    \n",
    "   \n",
    "    if y_pred == True:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "    \n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "df= pd.DataFrame()\n",
    "df['seq'] = predictdata[\"seq\"]\n",
    "df['predicted_class'] = predictdata['seq'].apply(predict_peptide_class)\n",
    "\n"
   ]
